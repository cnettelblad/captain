{
  "name": "captain",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "imports": {
    "#captain/*": "./dist/*"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "dev:build": "tsc -p tsconfig.json --watch",
    "dev:run": "node --watch dist/index.js",
    "dev": "concurrently -k \"npm:dev:build\" \"npm:dev:run\"",
    "start": "prisma generate && prisma migrate deploy && node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy-commands": "node dist/deploy-commands.js",
    "clear-guild-commands": "node dist/deploy-commands.js --clear-guild",
    "update-guild-command": "node dist/deploy-commands.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "discord-api-types": "^0.38.4",
    "discord.js": "^14.19.3",
    "dotenv": "^16.6.1",
    "node-cron": "^4.2.1",
    "prisma": "^7.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.19.5",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.23.1",
    "prettier": "^3.7.4",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  }
}
